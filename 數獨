#include<iostream>
#include <vector>
#include <algorithm>
using namespace std;
class Sudoku
{
private:
    vector<vector<int>> board;
public:
    Sudoku()
    {

    }
    void generate()
    {
        srand(time(0));
        board = vector<vector<int>>(9, vector<int>(9, 0));
        vector<int> nums = { 1, 2, 3, 4, 5, 6, 7, 8, 9 };
        random_shuffle(nums.begin(), nums.end());
        for (int i = 0; i < 9; i++)
        {
            for (int j = 0; j < 9; j++)
            {
                if (i % 3 == 0 && j % 3 == 0)
                {
                    int k = 0;
                    for (int x = i; x < i + 3; x++)
                    {
                        for (int y = j; y < j + 3; y++)
                        {
                            board[x][y] = nums[k++];
                        }
                    }
                    random_shuffle(nums.begin(), nums.end());
                }
            }
        }
        for (int i = 0; i < 9; i++)
        {
            for (int j = 0; j < 9; j++)
            {
                if (rand() % 2 == 0)
                {
                    board[i][j] = 0;
                }
            }
        }
        for (int i = 0; i < 9; i++)
        {
            for (int j = 0; j < 9; j++)
            {
                int val = board[i][j];
                if (val != 0)
                {
                    for (int k = j + 1; k < 9; k++)
                    {
                        if (board[i][k] == val)
                        {
                            board[i][k] = 0;
                        }
                    }
                }
            }
        }
        for (int j = 0; j < 9; j++)
        {
            for (int i = 0; i < 9; i++)
            {
                int val = board[i][j];
                if (val != 0)
                {
                    for (int k = i + 1; k < 9; k++)
                    {
                        if (board[k][j] == val)
                        {
                            board[k][j] = 0;
                        }
                    }
                }
            }
        }
    }
    void print() {
        for (int i = 0; i < 9; i++)
        {
            for (int j = 0; j < 9; j++)
            {
                cout << board[i][j] << " ";
            }
            cout << endl;
        }
    }
};
int main()
{
    Sudoku sudoku;
    sudoku.generate();
    sudoku.print();
    return 0;
}
bool isValid(int row,int col,int num)
{
    for(int i=0;i<9;i++)
    {
        if(board[row][i]=num) return flase;
    }
    for(int i=0;i<9;i++)
    {
        if(board[i][col]=num) return flase;
    }
    int box_row=row/3*3;
    int box_col=col/3*3;
    for(int i=box_row;i<box_row+3;i++)
    {
        for(int j=box_col;j<box_col+3;j++)
        {
            if(board[i][j]=num) return flase;
        }
    }
    return true;
}

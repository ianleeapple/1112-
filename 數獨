#include<iostream>
using namespace std;
class Sudoku
{
  private:
    vector<vector<int>> board;
  public:
         Sudoku()
        {

        }
        void generate() 
        {
          srand(time(0));
          board = vector<vector<int>>(9, vector<int>(9, 0));
          vector<int> nums = { 1, 2, 3, 4, 5, 6, 7, 8, 9 };
          random_shuffle(nums.begin(), nums.end());
          for (int i = 0; i < 9; i++) 
          {
              for (int j = 0; j < 9; j++) 
              {
                  if (i % 3 == 0 && j % 3 == 0) 
                  {
                      int k = 0;
                      for (int x = i; x < i + 3; x++) 
                      {
                          for (int y = j; y < j + 3; y++) 
                          {
                              board[x][y] = nums[k++];
                          }
                      }
                      random_shuffle(nums.begin(), nums.end());
                  }
              }
          }
          for (int i = 0; i < 9; i++) 
          {
              for (int j = 0; j < 9; j++) 
              {
                  if (rand() % 2 == 0) 
                  {
                      board[i][j] = 0;
                  }
              }
           }
       }
}
int main()
{

}

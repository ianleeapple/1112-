#include<iostream>
using namespace std;
class score
{
  private:
      int maze[9][9];
  public:
      void print()
      {
            for (int i=0; i<9;i++)
            {
               cout<<"|";
                for (int j=0;j<9;j++)
                {
                    cout<<maze[i][j];
                    if(j%3==2)
                    {
                      cout<<"|";
                    }
                }
               cout<<endl;
               if(i%3==2)
                {
                  cout<<"-------------"<<endl;
                }
            }
      }
};
int main()
{
  score score1;
  score1.print();
}
